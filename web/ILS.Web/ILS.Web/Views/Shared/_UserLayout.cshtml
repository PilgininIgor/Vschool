    <script type="text/javascript">



        var sdoorl = true;
        var sdoorr = true;

        var sdoorlTimer;
        var sdoorlLeft = 0;
        var sdoorrTimer;
        var sdoorrLeft = 0;

        function sdoor_open_l() {
            clearInterval(sdoorlTimer);
            function move() {
                sdoorlLeft -= 3;
                document.getElementById('sdoorll').style.left = sdoorlLeft + 'px';
                document.getElementById('sdoorlr').style.left = 43 - sdoorlLeft + 'px';
                if (sdoorlLeft <= -102) {
                    clearInterval(sdoorlTimer);
                }
            }
            sdoorlTimer = setInterval(move, 1)
        }

        function sdoor_open_r() {
            clearInterval(sdoorrTimer);
            function move() {
                sdoorrLeft -= 3;
                document.getElementById('sdoorrl').style.left = sdoorrLeft + 'px';
                document.getElementById('sdoorrr').style.left = 43 - sdoorrLeft + 'px';
                if (sdoorrLeft <= -102) {
                    clearInterval(sdoorrTimer);
                }
            }
            sdoorrTimer = setInterval(move, 1)
        }

        function sdoor_close_l() {
            clearInterval(sdoorlTimer);
            function move() {
                sdoorlLeft += 3;
                document.getElementById('sdoorll').style.left = sdoorlLeft + 'px';
                document.getElementById('sdoorlr').style.left = 43 - sdoorlLeft + 'px';
                if (sdoorlLeft >= 0) {
                    clearInterval(sdoorlTimer);
                }
            }
            sdoorlTimer = setInterval(move, 1);
        }


        function sdoor_close_r() {
            clearInterval(sdoorrTimer);
            function move() {
                sdoorrLeft += 3;
                document.getElementById('sdoorrl').style.left = sdoorrLeft + 'px';
                document.getElementById('sdoorrr').style.left = 43 - sdoorrLeft + 'px';
                if (sdoorrLeft >= 0) {
                    clearInterval(sdoorrTimer);
                }
            }
            sdoorrTimer = setInterval(move, 1);
        }

        function sdoor_force_l() {
            sdoor_close_l();
        }
        function sdoor_force_r() {
            sdoor_close_r();
        }

        var frameLeftIndex = 0;
        var frameLeftTimer;
        var framesLeftScreens;
        function changeLeftScreens() {
            switch (frameLeftIndex) {
                case 8:
                    framesLeftScreens[0].style.opacity = 0.9;
                    framesLeftScreens[1].style.opacity = 0.3;
                    break;
                case 9:
                    framesLeftScreens[0].style.opacity = 0.3;
                    framesLeftScreens[1].style.opacity = 0.9;
                    break;
                case 10:
                    framesLeftScreens[0].style.opacity = 0;
                    framesLeftScreens[1].style.opacity = 1;
                    break;
                case 18:
                    framesLeftScreens[1].style.opacity = 0.9;
                    framesLeftScreens[2].style.opacity = 0.3;
                    break;
                case 19:
                    framesLeftScreens[1].style.opacity = 0.3;
                    framesLeftScreens[2].style.opacity = 0.9;
                    break;
                case 20:
                    framesLeftScreens[1].style.opacity = 0;
                    framesLeftScreens[2].style.opacity = 1;
                    break;
                case 28:
                    framesLeftScreens[2].style.opacity = 0.9;
                    framesLeftScreens[3].style.opacity = 0.3;
                    break;
                case 29:
                    framesLeftScreens[2].style.opacity = 0.3;
                    framesLeftScreens[3].style.opacity = 0.9;
                    break;
                case 30:
                    framesLeftScreens[2].style.opacity = 0;
                    framesLeftScreens[3].style.opacity = 1;
                    break;
                case 38:
                    framesLeftScreens[3].style.opacity = 0.9;
                    framesLeftScreens[0].style.opacity = 0.3;
                    break;
                case 39:
                    framesLeftScreens[3].style.opacity = 0.3;
                    framesLeftScreens[0].style.opacity = 0.9;
                    break;
                case 40:
                    framesLeftScreens[3].style.opacity = 0;
                    framesLeftScreens[0].style.opacity = 1;
                    frameLeftIndex = 0;
                    break;
            }
            frameLeftIndex++;
        }

        var frameRightIndex = 0;
        var frameRightTimer;
        var framesRightScreens;
        function changeRightScreens() {
            switch (frameRightIndex) {
                case 8:
                    framesRightScreens[0].style.opacity = 0.9;
                    framesRightScreens[1].style.opacity = 0.3;
                    break;
                case 9:
                    framesRightScreens[0].style.opacity = 0.3;
                    framesRightScreens[1].style.opacity = 0.9;
                    break;
                case 10:
