                    framesRightScreens[1].style.opacity = 1;
                    break;
                case 18:
                    framesRightScreens[1].style.opacity = 0.9;
                    framesRightScreens[2].style.opacity = 0.3;
                    break;
                case 19:
                    framesRightScreens[1].style.opacity = 0.3;
                    framesRightScreens[2].style.opacity = 0.9;
                    break;
                case 20:
                    framesRightScreens[1].style.opacity = 0;
                    framesRightScreens[2].style.opacity = 1;
                    break;
                case 28:
                    framesRightScreens[2].style.opacity = 0.9;
                    framesRightScreens[3].style.opacity = 0.3;
                    break;
                case 29:
                    framesRightScreens[2].style.opacity = 0.3;
                    framesRightScreens[3].style.opacity = 0.9;
                    break;
                case 30:
                    framesRightScreens[2].style.opacity = 0;
                    framesRightScreens[3].style.opacity = 1;
                    break;
                case 38:
                    framesRightScreens[3].style.opacity = 0.9;
                    framesRightScreens[0].style.opacity = 0.3;
                    break;
                case 39:
                    framesRightScreens[3].style.opacity = 0.3;
                    framesRightScreens[0].style.opacity = 0.9;
                    break;
                case 40:
                    framesRightScreens[3].style.opacity = 0;
                    framesRightScreens[0].style.opacity = 1;
                    frameRightIndex = 0;
                    break;
            }
            frameRightIndex++;
        }

        var framel = 0;

        var demolTimer;

        function demol_animate() {
            clearInterval(demolTimer);
            var frames = document.getElementById("demolcontainer").children;
            function move() {
                frames[framel].style.display = "none";
                framel++;
                if (framel == 24) {
                    clearInterval(demolTimer);
                    framel = 23;

                }
                frames[framel].style.display = "block";

            }
            demolTimer = setInterval(move, 40);
            framesLeftScreens = document.getElementById("demolScreenshots").children;
            document.getElementById("demolScreenshots").style.display = "block";
            frameLeftTimer = setInterval(changeLeftScreens, 100);
        }

        function demol_stop() {
            clearInterval(demolTimer);
            var frames = document.getElementById("demolcontainer").children;
            function move() {
                if (framel < 1) {
                    clearInterval(demolTimer);
                    document.getElementById("demolScreenshots").style.display = "none";
                    clearInterval(frameLeftTimer);
                    framel = 1;
                }
                frames[framel].style.display = "none";
                framel--;
                frames[framel].style.display = "block";

            }
            demolTimer = setInterval(move, 40);
        }




        var framer = 0;

        var demorTimer;

        function demor_animate() {
            clearInterval(demorTimer);
            var frames = document.getElementById("demorcontainer").children;
            function move() {
                frames[framer].style.display = "none";
                framer++;
                if (framer == 24) {
                    clearInterval(demorTimer);
                    framer = 23;
                }
                frames[framer].style.display = "block";

            }
            demorTimer = setInterval(move, 40);
            framesRightScreens = document.getElementById("demorScreenshots").children;
            document.getElementById("demorScreenshots").style.display = "block";
            frameRightTimer = setInterval(changeRightScreens, 100);
        }

        function demor_stop() {
            clearInterval(demorTimer);
            var frames = document.getElementById("demorcontainer").children;
            function move() {
                if (framer < 1) {
                    clearInterval(demorTimer);
                    document.getElementById("demorScreenshots").style.display = "none";
                    clearInterval(frameRightTimer);
                    framer = 1;
                }
                frames[framer].style.display = "none";
                framer--;
                frames[framer].style.display = "block";

            }
            demorTimer = setInterval(move, 40);
        }

		var suspensionTimer;
		var suspensionInterval;
		var suspensionOpacity = 0;

		function showWaitingRing() {
			document.getElementById("waitingRing").style.display = "block";
			document.getElementById("waitingBlack").style.display = "block";
			suspensionInterval = setInterval(enhanceWaitingOpacity, 40);
		}

		function enhanceWaitingOpacity() {
			suspensionOpacity += 0.05;
			document.getElementById("waiting").style.opacity = suspensionOpacity;
			if (suspensionOpacity > 0.95) {
				clearInterval(suspensionInterval);
			}
		}

		function suspend() {
			suspensionOpacity = 0;
			clearTimeout(suspensionTimer);
			clearInterval(suspensionInterval);
			document.getElementById("waitingRing").style.display = "none";
			document.getElementById("waitingBlack").style.display = "none";
			document.getElementById("waiting").style.display = "block";
			document.getElementById("waiting").style.opacity = "0";
			suspensionTimer = setTimeout(showWaitingRing, 1000);
		}

		function resume() {
			clearTimeout(suspensionTimer);
			clearInterval(suspensionInterval);
			document.getElementById("waiting").style.display = "none";
			document.getElementById("waiting").style.opacity = "0";
			document.getElementById("waitingRing").style.display = "none";
			document.getElementById("waitingBlack").style.display = "none";
		}
    </script>



